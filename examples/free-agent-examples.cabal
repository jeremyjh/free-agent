name:                free-agent-examples
version:             0.1.0.0
-- synopsis:            
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Jeremy Huffman
maintainer:          jeremy@jeremyhuffman.com
-- copyright:           
category:            System
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable famon
  main-is:             Main.hs
  ghc-options:         -threaded -rtsopts -with-rtsopts=-N
  other-modules:       FreeAgent.Plugins.Nagios
  build-depends:       base >=4.6 && <4.7
                     , free-agent-core
                     , free-agent-cli
                     , aeson
                     , data-default
                     , unordered-containers
                     , process
  hs-source-dirs:      famon
  default-language:    Haskell2010
  ghc-prof-options: -O2 -auto-all

